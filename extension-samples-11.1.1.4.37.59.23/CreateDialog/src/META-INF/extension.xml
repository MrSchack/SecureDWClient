<extension id="oracle.ide.extsamples.createdialog" version="11.1.1"
           esdk-version="1.0" xmlns="http://jcp.org/jsr/198/extension-manifest">
  <name>Create Dialog</name>
  <owner>Oracle</owner>
  <dependencies>
    <import>oracle.ide</import>
  </dependencies>
  <classpaths>
    <classpath>../../jlib/jewt4.jar</classpath>
    <classpath>../../../oracle_common/modules/oracle.javatools_11.1.1/javatools-nodeps.jar</classpath>
    <classpath>../../ide/lib/oicons.jar</classpath>
    <classpath>../../ide/lib/javatools.jar</classpath>
    <classpath>../../ide/lib/uic.jar</classpath>
  </classpaths>
  <hooks>
    <feature-hook>
      <part-of>oracle.ide.extsamples.allsamples</part-of>
      <description>Demonstrates how to implement a simple Gallery Item that invokes a create dialog.</description>
      <optional>true</optional>
    </feature-hook>
    <jdeveloper-hook xmlns="http://xmlns.oracle.com/jdeveloper/1013/extension">
      <gallery>
        <item>
          <name>oracle.ide.extsamples.createdialog.PropertiesFileWizard</name>
          <category>Client Tier</category>
          <folder>General</folder>
        </item>
      </gallery>
    </jdeveloper-hook>
  </hooks>
</extension>
